{
  "compilerOptions": {
    "target": "ES2016",
    "module": "commonjs",
    "lib": ["esnext"],
    "strict": false,
    "rootDir": ".",
    "outDir": "dist",
    "sourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noEmitOnError": true,  
    "removeComments": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node"
  }
}